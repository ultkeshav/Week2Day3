class Solution {
  public:
    vector<int> postOrder(Node* root) {
    
        vector<int> result;
        postOrderUtil(root,result);
        return result;
        
    }
    
    void postOrderUtil(Node* root,vector<int>&result){
        
        if(!root) return;
        
        
        postOrderUtil(root->left,result);
        postOrderUtil(root->right,result);
        result.push_back(root->data);
    }
};
